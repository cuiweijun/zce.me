{
  "name": "zce.me",
  "version": "0.1.0",
  "private": true,
  "description": "zceâ€™s personal homepage",
  "keywords": [
    "homepage",
    "zce.me",
    "zce"
  ],
  "homepage": "https://github.com/zce/zce.me#readme",
  "bugs": {
    "url": "https://github.com/zce/zce.me/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/zce.me.git"
  },
  "license": "MIT",
  "author": {
    "name": "zce",
    "email": "w@zce.me",
    "url": "https://zce.me"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,yml,yaml,md,scss}\"",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "dist": "yarn clean && yarn build --prefix-paths",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "dependencies": {
    "@chakra-ui/core": "^0.4.1",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "disqus-react": "1.0.7",
    "emotion-theming": "^10.0.19",
    "gatsby": "2.17.7",
    "gatsby-image": "2.2.30",
    "gatsby-plugin-nprogress": "2.1.12",
    "gatsby-plugin-react-helmet": "3.1.13",
    "gatsby-plugin-sharp": "2.2.36",
    "gatsby-remark-autolink-headers": "2.1.16",
    "gatsby-remark-copy-linked-files": "2.1.28",
    "gatsby-remark-images": "3.1.28",
    "gatsby-remark-prismjs": "3.3.20",
    "gatsby-remark-responsive-iframe": "2.2.25",
    "gatsby-remark-smartypants": "2.1.14",
    "gatsby-source-filesystem": "2.1.35",
    "gatsby-transformer-remark": "2.6.32",
    "gatsby-transformer-sharp": "2.3.2",
    "hls.js": "0.12.4",
    "husky": "3.0.9",
    "js-yaml": "3.13.1",
    "lint-staged": "9.4.2",
    "lodash": "4.17.15",
    "pluralize": "8.0.0",
    "plyr": "3.5.6",
    "prettier": "1.18.2",
    "prismjs": "1.17.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-helmet": "5.2.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
