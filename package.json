{
  "name": "zce.me",
  "version": "0.1.17",
  "private": true,
  "description": "zceâ€™s personal homepage",
  "keywords": [
    "homepage",
    "zce.me",
    "zce"
  ],
  "homepage": "https://github.com/zce/zce.me#readme",
  "bugs": {
    "url": "https://github.com/zce/zce.me/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/zce.me.git"
  },
  "license": "MIT",
  "author": {
    "name": "zce",
    "email": "w@zce.me",
    "url": "https://zce.me"
  },
  "sideEffects": false,
  "scripts": {
    "new": "plop",
    "format": "prettier --write \"**/*.{js,json,yml,md}\"",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,json,yml,md}": "prettier --write"
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "renovate": {
    "extends": [
      "zce"
    ]
  },
  "dependencies": {
    "@emotion/babel-plugin-jsx-pragmatic": "0.1.5",
    "@emotion/core": "10.0.35",
    "emotion": "10.0.27",
    "emotion-server": "10.0.27",
    "emotion-theming": "10.0.27",
    "gatsby": "2.24.47",
    "gatsby-image": "2.4.16",
    "gatsby-plugin-manifest": "2.4.23",
    "gatsby-plugin-nprogress": "2.3.10",
    "gatsby-plugin-offline": "3.2.23",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.27",
    "gatsby-plugin-sitemap": "2.4.11",
    "gatsby-remark-autolink-headers": "2.3.11",
    "gatsby-remark-copy-linked-files": "2.3.11",
    "gatsby-remark-images": "3.3.25",
    "gatsby-remark-prismjs": "3.5.10",
    "gatsby-remark-responsive-iframe": "2.4.11",
    "gatsby-remark-smartypants": "2.3.10",
    "gatsby-source-filesystem": "2.3.24",
    "gatsby-transformer-remark": "2.8.28",
    "gatsby-transformer-sharp": "2.5.13",
    "html-minifier": "4.0.0",
    "pluralize": "8.0.0",
    "polished": "3.6.5",
    "prismjs": "1.21.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "plop": "2.7.4",
    "prettier": "2.0.5"
  },
  "engines": {
    "node": ">=10"
  }
}
