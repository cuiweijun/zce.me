{
  "name": "zce.me",
  "version": "0.1.0",
  "private": true,
  "description": "zceâ€™s personal homepage",
  "keywords": [
    "homepage",
    "zce.me",
    "zce"
  ],
  "homepage": "https://github.com/zce/zce.me#readme",
  "bugs": {
    "url": "https://github.com/zce/zce.me/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zce/zce.me.git"
  },
  "license": "MIT",
  "author": {
    "name": "zce",
    "email": "w@zce.me",
    "url": "https://zce.me"
  },
  "sideEffects": false,
  "scripts": {
    "new": "plop",
    "format": "prettier --write \"**/*.{js,json,yml,yaml,md}\"",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "dev": "yarn develop",
    "dist": "yarn clean && yarn build --prefix-paths",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2
  },
  "dependencies": {
    "@emotion/babel-plugin-jsx-pragmatic": "0.1.5",
    "@emotion/core": "10.0.27",
    "emotion": "10.0.27",
    "emotion-server": "10.0.27",
    "emotion-theming": "10.0.27",
    "gatsby": "2.18.17",
    "gatsby-image": "2.2.37",
    "gatsby-plugin-manifest": "2.2.34",
    "gatsby-plugin-minify": "0.2.0",
    "gatsby-plugin-nprogress": "2.1.17",
    "gatsby-plugin-offline": "3.0.30",
    "gatsby-plugin-react-helmet": "3.1.18",
    "gatsby-plugin-sharp": "2.3.10",
    "gatsby-remark-autolink-headers": "2.1.21",
    "gatsby-remark-copy-linked-files": "2.1.33",
    "gatsby-remark-images": "3.1.39",
    "gatsby-remark-prismjs": "3.3.28",
    "gatsby-remark-responsive-iframe": "2.2.30",
    "gatsby-remark-smartypants": "2.1.19",
    "gatsby-source-filesystem": "2.1.43",
    "gatsby-transformer-remark": "2.6.45",
    "gatsby-transformer-sharp": "2.3.9",
    "pluralize": "8.0.0",
    "polished": "3.4.2",
    "prismjs": "1.18.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "plop": "2.5.3",
    "prettier": "1.19.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
